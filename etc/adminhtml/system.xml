<?xml version="1.0" ?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Config:etc/system_file.xsd">
	<system>
		<tab id="oneo" translate="label" sortOrder="999">
			<label>oneo</label>
		</tab>
		<section id="oneo" sortOrder="10" showInWebsite="1" showInStore="1" showInDefault="1" translate="label">
			<label>oneo</label>
			<tab>oneo</tab>
			<resource>OneO_Shop::config_oneo_shop</resource>
			<group id="general" sortOrder="10" showInWebsite="1" showInStore="1" showInDefault="1" translate="label">
				<label>general</label>
				<field id="key_id" type="text" sortOrder="10" showInWebsite="1" showInStore="1" showInDefault="1" translate="label">
					<label>key_id</label>
					<comment/>
				</field>
				<field id="shared_secret" type="obscure" sortOrder="10" showInWebsite="1" showInStore="1" showInDefault="1" translate="label">
					<label>shared_secret</label>
					<comment/>
					<backend_model>Magento\Config\Model\Config\Backend\Encrypted</backend_model>
					<config_path>oneo/general/shared_secret</config_path>
				</field>
			</group>
		</section>
	</system>
</config>
